{
	"name": "pubmed-search",
	"description": "tries searching pubmed",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"https://ajax.googleapis.com/",
		"https://www.google.com/",
		"http://www.ncbi.nlm.nih.gov/pubmed/"
	]
}